{
    "name": "oo-gui",
    "version": "0.1.0",
    "description": "An object oriented wrapper of lil-gui",
    "main": "dist/oo-gui.min.js",
    "directories": {
        "example": "examples",
        "test": "test"
    },
    "scripts": {
        "dist": "es-pack build -d ./dist -m umd esm esm-compat",
        "build": "es-pack build -m umd",
        "build:esm": "es-pack build -m esm",
        "build:esm:compat": "es-pack build -m esm-compat",
        "test": "npm run test:verify && npm run test:main",
        "test:verify": "es-pack build -m umd esm esm-compat --verify",
        "test:main": "es-pack test --node --browser",
        "dev": "es-pack build --dev"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/w3reality/oo-gui.git"
    },
    "keywords": [
        "lil-gui",
        "wrapper"
    ],
    "author": "j-devel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/w3reality/oo-gui/issues"
    },
    "homepage": "https://github.com/w3reality/oo-gui",
    "devDependencies": {
        "es-pack-js": "0.5.8",
        "lil-gui": "^0.16.1"
    }
}
